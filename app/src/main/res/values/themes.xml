<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AndoridTestBilalQwatly" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_grayF0F0F0</item>
        <item name="colorPrimaryVariant">@color/color_grayF0F0F0</item>
        <item name="colorOnPrimary">@color/color_grayF0F0F0</item>
        <item name="colorControlHighlight">@color/color_grayF0F0F0</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/color_grayF0F0F0</item>
        <item name="android:navigationBarColor">@color/color_grayF0F0F0</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>

        <!-- Customize your theme here. -->
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>

        <item name="android:windowBackground">@color/color_grayF0F0F0</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:autofilledHighlight" tools:targetApi="o">@android:color/transparent
        </item>
    </style>

    <!-- Splash Theme -->
    <style name="SplashTheme" parent="Theme.AndoridTestBilalQwatly">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>


    <!-- Text Style-->
    <style name="MyTextStyle" parent="TextAppearance.AppCompat.Medium">
        <item name="android:background">@drawable/bg_ripple_text</item>
    </style>


    <!-- Poppins -->

    <style name="Text_Poppins_R_13_7B7B7B" parent="MyTextStyle">
        <item name="android:textColor">@color/color_gray7B7B7B</item>
        <item name="android:textSize">@dimen/font_13</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>


    <style name="Text_Poppins_R_14_7B7B7B" parent="MyTextStyle">
        <item name="android:textColor">@color/color_gray7B7B7B</item>
        <item name="android:textSize">@dimen/font_14</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>

    <style name="Text_Poppins_R_16_7B7B7B" parent="MyTextStyle">
        <item name="android:textColor">@color/color_gray7B7B7B</item>
        <item name="android:textSize">@dimen/font_16</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>


    <style name="Text_Poppins_R_14_212121" parent="MyTextStyle">
        <item name="android:textColor">@color/color_black212121</item>
        <item name="android:textSize">@dimen/font_14</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>

    <style name="Text_Poppins_R_16_212121" parent="MyTextStyle">
        <item name="android:textColor">@color/color_black212121</item>
        <item name="android:textSize">@dimen/font_16</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>

    <style name="Text_Poppins_M_18_FFFFFF" parent="MyTextStyle">
        <item name="android:textColor">@color/color_FFFFFF</item>
        <item name="android:textSize">@dimen/font_18</item>
        <item name="fontFamily">@font/poppins_medium</item>
    </style>

    <style name="TryAgainButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">@dimen/font_16</item>
        <item name="android:textColor">@color/color_FFFFFF</item>
        <item name="cornerRadius">@dimen/radius_11</item>
        <item name="backgroundTint">@color/color_gray7B7B7B</item>
        <item name="textAllCaps">false</item>
        <item name="android:paddingStart">@dimen/spacing_margin</item>
        <item name="android:paddingEnd">@dimen/spacing_margin</item>
        <item name="android:paddingTop">@dimen/spacing_small</item>
        <item name="android:paddingBottom">@dimen/spacing_small</item>
    </style>

</resources>